function toggle1(){
var elemento1; elemento1=document.querySelector("#attdis");
elemento1.style.visibility="visible";
var elemento11; elemento11=document.querySelector("#toggle1");
elemento11.style.visibility="hidden";
var elemento12; elemento12=document.querySelector("#toggle2");
elemento12.style.visibility="visible";


}

